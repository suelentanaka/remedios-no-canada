<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.campos.bianca.remediosnocanada">

    <application
        android:allowBackup="true"
        android:icon="@drawable/rc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailsActivity"></activity>

        <meta-data
            android:name="DATABASE"
            android:value="remediosnocanada.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.campos.bianca.remediosnocanada" />

        <activity android:name=".FavoritesActivity"></activity>
    </application>

</manifest>